<UserControl x:Class="Accidis.Apps.Preflight.Views.Main.ChartsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             d:DesignHeight="445" 
             d:DesignWidth="480">

	<Grid Height="400" VerticalAlignment="Top">
		<Grid.ColumnDefinitions>
			<ColumnDefinition />
			<ColumnDefinition />
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition />
			<RowDefinition />
			<RowDefinition />
		</Grid.RowDefinitions>
		<Button Click="OnNordicSwcClicked" Grid.ColumnSpan="2">Nordic SWC</Button>
		<Button Click="OnVfrChartClicked" Grid.Row="1" Grid.Column="0">VFR Chart</Button>
		<Button Click="OnVfrFc06Clicked" Grid.Row="1" Grid.Column="1">VFR FC 06</Button>
		<Button Click="OnLlfChartClicked" Grid.Row="2" Grid.Column="0">LLF Chart</Button>
		<Button Click="OnVfrFc12Clicked" Grid.Row="2" Grid.Column="1">VFR FC 12</Button>
	</Grid>
</UserControl>